class Solution {
public:
    void sortColors(vector<int>& nums) {
    int begin=0,end=nums.size()-1;
  int i=0;
        while(i<=end)
        {   
        if(nums[i]==0)
        {
            nums[i]=nums[begin];
            nums[begin++]=0;
            i++;
        } 
        else if(nums[i]==2)
        {
            nums[i]=nums[end];
            nums[end]=2;
            end--;
        }
            else 
                i++;
       }
        
    }
};//学习快排~
